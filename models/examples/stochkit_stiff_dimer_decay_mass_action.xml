<?xml version="1.0"?>
<!-- Stochkit 2.0 text input format -->

<!--
   different parameters for different types of reactions:
     <Rate> for mass-action
     <Vmax> <Km> for michaelis-menten
     <PropensityFunction> for cunstomized propensity function
-->

<Model>
   <Description>Stiff Dimer Decay</Description>
   <NumberOfReactions>4</NumberOfReactions>
   <NumberOfSpecies>3</NumberOfSpecies>
   <ParametersList>
     <Parameter>
       <Id>R1</Id>
       <Expression>1.0</Expression>
     </Parameter>
     <Parameter>
       <Id>R2</Id>
       <Expression>10.0</Expression>
     </Parameter>
     <Parameter>
       <Id>R3</Id>
       <Expression>1000.0</Expression>
     </Parameter>
     <Parameter>
       <Id>R4</Id>
       <Expression>0.1</Expression>
     </Parameter>
   </ParametersList>
   <ReactionsList>
     <Reaction>
       <Id>A1</Id>
       <Description> S1 -> null </Description>
       <Type>mass-action</Type>
       <Rate>R1</Rate> 
       <Reactants>
           <SpeciesReference id="S1" stoichiometry="1"/>
       </Reactants>
       <Products>
       </Products>  <!-- no product -->
     </Reaction>
     <Reaction>
       <Id>A2</Id>
       <Description> 2 * S1 -> S2 </Description>
       <Type>mass-action</Type>
       <Rate>R2</Rate>
       <Reactants>
           <SpeciesReference id="S1" stoichiometry="2"/>
       </Reactants>
       <Products>
           <SpeciesReference id="S2" stoichiometry="1"/>
       </Products>
     </Reaction>
     <Reaction>
       <Id>A3</Id>
       <Description> S2 -> 2 * S1 </Description>
       <Type>mass-action</Type>
       <Rate>R3</Rate>
       <Reactants>
           <SpeciesReference id="S2" stoichiometry="1"/>
       </Reactants>
       <Products>
           <SpeciesReference id="S1" stoichiometry="2"/>
       </Products>
     </Reaction>
     <Reaction>
       <Id>A4</Id>
       <Description> S2 -> S3 </Description>
       <Type>mass-action</Type>
       <Rate>R4</Rate>
       <Reactants>
           <SpeciesReference id="S2" stoichiometry="1"/>
       </Reactants>
       <Products>
           <SpeciesReference id="S3" stoichiometry="1"/>
       </Products>
     </Reaction>
  </ReactionsList>
  <SpeciesList>
     <Species>
       <Id>S1</Id>
       <Description>Species #1</Description>
       <InitialPopulation>400</InitialPopulation>
     </Species>
     <Species>
       <Id>S2</Id>
       <Description>Species #2</Description>
       <InitialPopulation>798</InitialPopulation>
     </Species>
     <Species>
       <Id>S3</Id>
       <Description>Species #3</Description>
       <InitialPopulation>0</InitialPopulation>
     </Species>
  </SpeciesList>
</Model>
